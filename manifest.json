  "screenshots": [
    {
      "src": "assets/images/screenshot-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "ThinkBit Edge homepage featuring AI research and data center dashboard"
    },
    {
      "src": "assets/images/screenshot-narrow.png",
      "sizes": "640x1136",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile view of ThinkBit Edge with responsive design and interactive features"
    }
  ],
  "features": [
    "background-sync",
    "push-messaging",
    "offline-support"
  ],
  "serviceworker": {
    "src": "sw.js",
    "scope": "/",
    "update_via_cache": "none"
  },
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "protocol_handlers": [
    {
      "protocol": "web+thinkbitedge",
      "url": "/?action=%s"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "file_handlers": [
    {
      "action": "/handle-csv",
      "accept": {
        "text/csv": [".csv"]
      },
      "icons": [
        {
          "src": "assets/images/csv-icon.png",
          "sizes": "256x256",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    },
    {
      "action": "/handle-data",
      "accept": {
        "application/json": [".json"],
        "text/plain": [".txt"]
      },
      "icons": [
        {
          "src": "assets/images/data-icon.png",
          "sizes": "256x256",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "share_target": {
    "action": "/share-data",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "data",
          "accept": [
            "text/csv",
            "application/json",
            "text/plain"
          ]
        }
      ]
    }
  },
  "display_override": [
    "window-controls-overlay",
    "minimal-ui"
  ],
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "permissions_policy": {
    "camera": [],
    "microphone": [],
    "geolocation": ["self"],
    "notifications": ["self"]
  }
}